// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model Auth {
  access_token  String
  refresh_token String @unique
}

model ShareLinks {
  code      String @unique
  directory String
}
